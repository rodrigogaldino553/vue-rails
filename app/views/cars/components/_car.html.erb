<div id="<%= dom_id car %>">
  <p>
    <strong>Name:</strong>
    <input type="text" v-model="car.name">
    {{ car.name }}
    <button @click="saveCar">save</button>
  </p>

  <p>
    <strong>Brand:</strong>
    <%= car.brand %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= car.price %>
  </p>

</div>
